{
  "scripts": {
    "build": "babel src --out-dir dist --ignore \"src/**/**/__tests__/\"",
    "serve": "node dist/index.js",
    "restart": "rimraf dist && yarn build && yarn serve",
    "dev": "nodemon --exec yarn restart",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --list-different \"src/**/*.js\"",
    "lint": "eslint \"src/**/{*.js,}\" --quiet --fix",
    "validate": "yarn run format:check && yarn run lint",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "cat ./coverage/lcov.info | codecov"
  },
  "name": "footballers",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Pablo Lorenzo",
  "license": "GPL-3.0",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "d3-dsv": "^2.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "uuid": "^8.3.1",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.5.2",
    "chalk": "^4.0.0",
    "codecov": "^3.5.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.1.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.5.2",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.0",
    "supertest": "^5.0.0"
  }
}
