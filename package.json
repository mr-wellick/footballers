{
  "scripts": {
    "serve": "node --env-file=.env dist/server.js",
    "build": "tsc",
    "restart": "rimraf dist && npm run build && npm run serve",
    "dev": "nodemon --watch src --exec npm run restart",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --list-different \"src/**/*.ts\"",
    "lint": "tsc --noEmit true",
    "test:watch": "node --import tsx --test --watch src/**/*.test.ts"
  },
  "name": "footballers",
  "version": "1.0.0",
  "main": "dist/server.js",
  "author": "Mr. Wellick",
  "license": "GPL-3.0",
  "type": "module",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "d3-dsv": "^3.0.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "supertest": "^6.3.4",
    "uuid": "^9.0.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/d3-dsv": "^3.0.7",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "chalk": "^5.3.0",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
